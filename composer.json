{
    "name": "laravel-interaction/bookmark",
    "description": "User bookmark/unbookmark behaviour for Laravel.",
    "keywords": ["laravel", "bookmarks", "bookmark"],
    "license": "MIT",
    "authors": [
        {
            "name": "zingimmick",
            "email": "zingimmick@outlook.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "require": {
        "php": "^7.2 || ^8.0",
        "illuminate/database": "^6.20.12 || ^7.30.3 || ^8.22.1",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.3.3 || ^1.4.2",
        "moontoast/math": "^1.0",
        "orchestra/testbench": "^4.0 || ^5.0 || ^6.0",
        "phpunit/phpunit": "^8.0,<8.5.12 || ^9.3.3",
        "zing/coding-standard": "^2.6 || ^3.2"
    },
    "autoload": {
        "psr-4": {
            "Zing\\LaravelBookmark\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zing\\LaravelBookmark\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zing\\LaravelBookmark\\BookmarkServiceProvider"
            ]
        }
    },
    "scripts": {
        "lint:rector": "@fix:rector --dry-run",
        "lint:ecs": "ecs check --ansi",
        "fix:rector": "rector process --ansi",
        "fix:ecs": "@lint:ecs --fix",
        "test:phpunit": "phpunit --colors=always",
        "lint": [
            "@lint:rector",
            "@lint:ecs"
        ],
        "fix": [
            "@fix:rector",
            "@fix:ecs"
        ],
        "test": [
            "@test:phpunit"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
